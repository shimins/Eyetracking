<Window x:Class="WPF.MenyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MenyWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="NewUserTestButton" Content="New User UserTest" HorizontalAlignment="Left" Margin="68,96,0,0" VerticalAlignment="Top" Width="152" Click="NewUserTestButton_OnClick" Height="52" FontSize="18.667" IsEnabled="False"/>
        <Button x:Name="DeveloperButton" Content="Developer Mode" HorizontalAlignment="Left" Margin="87,212,0,0" VerticalAlignment="Top" Width="116" Click="DeveloperButton_OnClick" Height="43"/>
        <TextBox x:Name="TesterName" HorizontalAlignment="Left" Height="30" Margin="132,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143" FontSize="16" TextChanged="TesterName_OnTextChanged"/>
        <Label x:Name="TesterNameLabel" Content="Tester Name:" HorizontalAlignment="Left" Height="30" Margin="19,50,0,0" VerticalAlignment="Top" Width="108" RenderTransformOrigin="0.686,1.098" FontSize="16"/>

    </Grid>
</Window>
